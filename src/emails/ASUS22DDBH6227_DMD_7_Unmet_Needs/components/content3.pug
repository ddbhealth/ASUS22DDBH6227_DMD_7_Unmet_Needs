table(width="600" align="center" class="mobile-table" bgcolor="#ffffff")&attributes(tableAtt)
  tr 
    td 
      table(width="100%" align="center" class="mobile-table")&attributes(tableAtt)
        tr
          +hSpacer(30).mobile-hide
          +hSpacer(10).mobile-show
          td 
            table(width="100%" align="center" class="mobile-table" bgcolor="#EDEEEF")&attributes(tableAtt)
              +vSpacer(15)
              tr
                td 
                  table(width="100%" align="center" class="mobile-table")&attributes(tableAtt)
                    tr
                      +hSpacer(10)
                      td(align="left" valign="top")
                        table(width="100%" align="center" class="mobile-table")&attributes(tableAtt)
                          tr 
                            td(align="center" valign="top")
                                font(class="f14 tuna").
                                    !{content3.text1} 
                          +vSpacer(20)
                          tr 
                            td(align="center" valign="top").mobile-hide
                              table(width="100%" align="center" class="mobile-table mobile-hide")&attributes(tableAtt)
                                tr
                                  td(align="center" valign="top" width="80").mobile-hide
                                  td(align="center" valign="top").mobile-hide
                                      img(src=img_path+"55.png" alt="55" width="154")
                                  td(align="center" valign="top" width="35").mobile-hide
                                  td(align="left" valign="middle")
                                      font(class="f12 tuna").
                                          !{content3.text2}   
                                  td(align="center" valign="top" width="60").mobile-hide
                            td(align="center" valign="top").mobile-show
                              table(width="100%" align="center" class="mobile-table mobile-show")&attributes(tableAtt)
                                tr
                                  +hSpacer(25)
                                  td(align="center" valign="top")
                                      img(src=img_path+"55.png" alt="55" width="124")
                                  +hSpacer(15)
                                  td(align="left" valign="middle")
                                      font(class="f12").
                                            !{content3.text2}
                                  +hSpacer(15)
                      +hSpacer(10)
              +vSpacer(20)
          +hSpacer(30).mobile-hide
          +hSpacer(10).mobile-show